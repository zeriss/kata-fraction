{
  "name": "fraction-kata",
  "version": "1.0.0",
  "description": "Kata Fraction",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "pretest": "npm run lint",
    "watch": "jest --watchAll",
    "coverage": "jest --coverage",
    "build": "tsc"
  },
  "author": "Yoann Aubry <nimblydev@gmail.com>",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "jest": "^29.7.0",
    "prettier": "^3.3.3",
    "ts-jest": "^29.1.5",
    "tslint": "^6.1.3"
  },
  "jest": {
    "collectCoverage": true,
    "coverageDirectory": "coverage",
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js"
    ],
    "transform": {
      "\\.(ts|tsx)$": "ts-jest"
    },
    "testRegex": "(/test/*.spec.*|(\\.|/)(test|spec))\\.(js?|ts?)$"
  },
  "keywords": [
    "kata"
  ],
  "bugs": {},
  "dependencies": {
    "typescript": "^5.5.0"
  }
}